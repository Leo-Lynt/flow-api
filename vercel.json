{
  "version": 2,
  "installCommand": "npm install --package-lock-only --legacy-peer-deps && npm ci --legacy-peer-deps",
  "buildCommand": "echo 'Build complete'",
  "routes": [
    {
      "src": "/api/docs/swagger-ui.*",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
